{
  "name": "custom-typescript-electron-template",
  "version": "1.0.0",
  "main": "./dist/main/src/main.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clear": "yarn run rimraf [\"./dist\"]",
    "copy-renderer-static": "yarn run copyfiles -u 3 \"./proj/renderer/src/**/*.html\" \"./proj/renderer/src/**/*.css\" out \"./dist/renderer/src\"",
    "copy-main-assets": "yarn run copyfiles -u 3 \"./proj/main/assets/**/*\" out \"./dist/main/assets\"",
    "compile": "yarn run clear && yarn run copy-main-assets && yarn run copy-renderer-static && yarn run tsc --build \"./proj/main/tsconfig.json\" \"./proj/renderer/tsconfig.json\"",
    "start": "yarn run compile && cross-env DEBUG=true electron ."
  },
  "devDependencies": {
    "@types/node": "^17.0.7",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^16.0.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
