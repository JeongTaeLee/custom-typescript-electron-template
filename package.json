{
  "name": "custom-typescript-electron-template",
  "version": "1.0.0",
  "main": "./electron-main/dist/src/main.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clear": "yarn run rimraf \"./electron-main/dist\" \"./electron-renderer/dist\"",
    "copy-renderer-static": "copyfiles -u 1 \"./electron-renderer/src/**/*.html\" \"./electron-renderer/src/**/*.css\" out \"./electron-renderer/dist\"",
    "compile": "yarn run clear && yarn run copy-renderer-static && yarn run tsc --build \"./electron-main/tsconfig.json\" \"./electron-renderer/tsconfig.json\"",
    "start": "yarn run compile && cross-env DEBUG=true electron . && exit 0"
  },
  "devDependencies": {
    "@types/node": "^17.0.7",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^16.0.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
